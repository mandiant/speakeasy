            },
    
            {
                "name": "volmgr",
                "base_addr": "0xC6000000",
                "path": "C:\\Windows\\system32\\drivers\\volmgr.sys",
                "driver": {
                    "name": "\\Driver\\volmgr",
                    "devices": [
                        {
                            "name": "\\Device\\HarddiskVolume1"
                        }
                    ]
                }
            },
    
            {
                "name": "disk",
                "base_addr": "0xC3000000",
                "path": "C:\\Windows\\system32\\drivers\\disk.sys",
                "driver": {
                    "name": "\\Driver\\Disk",
